<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <entry key="osprocessrunner.logger.prefix">process.</entry>
    <entry key="osprocessrunner.stream.encoding">cp1257</entry>
    <entry key="useinternalschema">true</entry>
    <entry key="loader.batchsize">1000</entry>
    <entry key="read.impl">org.mmx.xdtl.runtime.command.JdbcReadCmd</entry>
<!--
    A comma separated list of directories of javascript files to execute at startup.
    A relative path is relative to xdtl home directory - usually $(HOME)/.xdtl.
    <entry key="startup.path">startup</entry>
 -->
<!--
    A comma separated list of root url-s of velocity resources. Velocity
    resources are loaded from package's directory first and if not found, then
    following directories are tried. Relative directories are relative to xdtl
    home directory - usually $(HOME)/.xdtl.
    <entry key="velocity.path">velocity</entry>
 -->
<!--
    A comma separated list of directories of extension packages. A relative
    path is relative to xdtl home directory - usually $(HOME)/.xdtl.
    <entry key="extensions.path">extensions</entry>
-->
<!--
    A comma separated list of directories of library packages. A relative
    path is relative to xdtl home directory - usually $(HOME)/.xdtl.
    <entry key="library.path">library</entry>
-->
<!--
	<entry key="write.impl">org.mmx.xdtl.runtime.command.PostgresqlWriteCmd</entry>
-->
<!--
    Exec based versions of read/write commands

    <entry key="read.impl">org.mmx.xdtl.runtime.command.ReadCmd</entry>
    <entry key="write.impl">org.mmx.xdtl.runtime.command.WriteCmd</entry> 
-->
<!--
    Generic JDBC based read command
 
    <entry key="read.impl">org.mmx.xdtl.runtime.command.JdbcReadCmd</entry>
 -->
<!--
    Read command for Oracle
 
    <entry key="read.impl">org.mmx.xdtl.runtime.command.OracleReadCmd</entry>
 -->
</properties>
