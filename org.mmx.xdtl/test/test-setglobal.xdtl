<?xml version="1.0" encoding="UTF-8"?>
<!--
	Overwrites global variable 'get'.
 -->
<package name="test-setglobal" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xdtl.org/xdtl xdtl.xsd" xmlns="http://xdtl.org/xdtl">
  <connection name="conn" type="FILE">xdtl:connection</connection>
  <tasks>
    <task name="first">
      <steps>
        <send source="overwritten" target="get" overwrite="1"/>
      </steps>
    </task>
  </tasks>
</package>
