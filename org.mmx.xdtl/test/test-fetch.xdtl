<?xml version="1.0" encoding="UTF-8"?>
<!--
	Fetch element test.
 -->
<package name="test-fetch" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xdtl.org/xdtl xdtl.xsd" xmlns="http://xdtl.org/xdtl">
  <variable name="table">test</variable>
  <connection name="conn" type="DB">$testdb</connection>
  <tasks>
    <task name="first" connection="$conn">
      <steps>
        <fetch source="select * from $table" type="CSV" target="/temp/test.csv" encoding="UTF-8" overwrite="1" connection="$conn"/>
        <read source="/temp/test.csv" type="CSV" target="$table" overwrite="1"/>
      </steps>
    </task>
  </tasks>
</package>
